{
  "name": "drop-drop-box",
  "version": "1.0.0",
  "description": "move drop box to google photos",
  "main": "./lib/index.js",
  "type": "commonjs",
  "bin": {
    "pizza": "./lib/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
    "create": "yarn run build && yarn run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && pizza"
  },
  "repository": "https://github.com/ndp/drop-drop-box",
  "author": "Andrew Peterson <andy@ndpsoftware.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/chalk": "*",
    "@types/figlet": "*",
    "@types/node": "*",
    "@types/node-fetch": "*",
    "nodemon": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "clear": "*",
    "commander": "*",
    "dotenv": "*",
    "dropbox": "*",
    "figlet": "*"
  }
}
