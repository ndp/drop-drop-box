{
  "name": "drop-drop-box",
  "version": "0.0.0",
  "description": "Move dropbox files to Google Drive ",
  "author": "Andy Peterson @ndp-software",
  "bin": {
    "drop-drop-box": "./bin/run"
  },
  "homepage": "https://github.com/https://github.com/ndp/drop-drop-box/drop-drop-box",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/ndp/drop-drop-box/drop-drop-box",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "*",
    "@oclif/plugin-help": "*",
    "@oclif/plugin-plugins": "*",
    "dotenv": "*"
  },
  "devDependencies": {
    "@oclif/test": "*",
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-config-oclif": "*",
    "eslint-config-oclif-typescript": "*",
    "mocha": "*",
    "oclif": "*",
    "shx": "*",
    "ts-node": "*",
    "tslib": "*",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "drop-drop-box",
    "dirname": "drop-drop-box",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/https://github.com/ndp/drop-drop-box/drop-drop-box/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
